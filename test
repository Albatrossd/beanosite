        <script>
            function play() {

                var number = Math.floor(Math.random() * (3) + 1);
                var audio = document.getElementById("audio".concat(String.toString(number)));
                audio.play();
            }
        </script>
        <input type="image" src="beano.jpg" style="width:100%" onclick="play()">
